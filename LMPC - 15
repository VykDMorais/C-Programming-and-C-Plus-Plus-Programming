#include <stdio.h>
#include <string.h>

int main(){
    char str1[100], str2[100], str3[100], str4[100];
    printf ("Escreva uma string\n");
    gets (str1);
    printf ("Escreva uma string\n");
    gets (str2);
    printf ("Escreva uma string\n");
    gets (str3);
    printf ("Escreva uma string\n");
    gets (str4);
    strcat (str2, str1);
    strcat (str3, str2);
    strcat (str4, str3);
    printf (str4);
    return (0);
}
