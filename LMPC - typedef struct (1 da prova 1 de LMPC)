#include <stdio.h>
#include <math.h>

 typedef struct{
    float x;
    float y;
 } vec_v;
vec_v vec;

 float resultante (vec_v vec){
    float r;
    r = sqrt (vec.x*vec.x+ vec.y*vec.y);
    return (r);
 }
 
 int main(){
    float resultado;
    printf ("Digite um valor\n");
    scanf ("%f", &vec.x);
    printf ("Digite outro valor\n");
    scanf ("%f", &vec.y);
    resultado = resultante (vec);
    printf ("O resultado vetorial de x e y Ã©: %f", resultado);
    return (0);
 }
